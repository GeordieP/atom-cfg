"*":
  "atom-ide-ui":
    "atom-ide-terminal":
      lineHeight: 1.2
      rendererType: "canvas"
  "autocomplete-plus":
    autoActivationDelay: 100
    useCoreMovementCommands: false
  core:
    autoHideMenuBar: true
    closeEmptyWindows: false
    disabledPackages: [
      "archive-view"
      "autoflow"
      "autosave"
      "background-tips"
      "bookmarks"
      "dev-live-reload"
      "find-and-replace"
      "go-to-line"
      "image-view"
      "language-c"
      "language-clojure"
      "language-coffee-script"
      "language-csharp"
      "language-java"
      "language-objective-c"
      "language-php"
      "language-perl"
      "open-on-github"
      "package-generator"
      "spell-check"
    ]
    telemetryConsent: "limited"
    themes: [
      "minima-gp-ui"
      "gmacs-blue-syntax"
    ]
    titleBar: "custom"
    useTreeSitterParsers: true
  "custom-title":
    template: "<%= fileName %><% if (projectPath) { %> - <%= projectPath %><% } %> - Atom <%= atom.getVersion() %>"
  editor:
    fontFamily: "Source Code Variable, Dank Mono, Fira Mono for Powerline, Input, DejaVu Sans Mono, Menlo, monospace"
    fontSize: 16
    invisibles: {}
    lineHeight: 1.7
    scrollPastEnd: true
    showIndentGuide: true
    zoomFontWhenCtrlScrolling: false
  "exception-reporting":
    userId: "e935c70a-de60-4131-9482-2f8641db8124"
  "markdown-preview": {}
  react:
    enabledForAllJavascriptFiles: true
  "status-bar": {}
  "vim-mode-plus":
    highlightSearch: false
    useSmartcaseForFind: true
    useSmartcaseForSearch: true
    useSmartcaseForSearchCurrentWord: true
  welcome:
    showOnStartup: false
